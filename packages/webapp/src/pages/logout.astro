---
import AuthLayout from '@layouts/AuthLayout.astro';
import Layout from '@layouts/Layout.astro';
---

<AuthLayout title="logging out...">
  <p>logging out...</p>
  <script>
    const res = await fetch('/api/auth/logout');

    if (res.status === 200) {
      window.location.href = '/login';
    }
  </script>
</AuthLayout>
